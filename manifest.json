{
  "manifest_version": 2,
  "name": "Youtube Player",
  "version": "0.1",


  "browser_action": {
    "default_popup":"popup.html",
    "default_icon": "icon.png"
  },

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.1.1.min.js","content.js"],
      "run_at": "document_start",
     "all_frames": true
    }
  ],

 "permissions": [
   "tabs",
   "<all_urls>"
 ],
  "background": {
    "scripts": ["jquery-3.1.1.min.js","background.js"]
  }
}
